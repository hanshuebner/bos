  <!-- Client side setup -->
  <clients>
    <client host="localhost" use_controller_vm="true" maxusers="15000"/>
  </clients>
  
  <!-- Server side setup -->
  <servers>
    <server host="jasmin.headcraft.de" port="80" type="tcp"></server>
  </servers>

  <!-- to start os monitoring (cpu, network, memory). Use an erlang
       agent on the remote machine or SNMP. erlang is the default --> 
  <!--   <monitoring> -->
  <!--     <monitor host="myserver" type="snmp"></monitor> -->
  <!--   </monitoring> -->
  
  <!-- several arrival phases can be set: for each phase, you can set
       the mean inter-arrival time between new clients and the phase
       duration -->

  <load>
    <arrivalphase phase="1" duration="10" unit="minute">
      <users interarrival="2" unit="second" maxnumber="500"></users>
    </arrivalphase>


    <arrivalphase phase="2" duration="5" unit="minute">
      <users interarrival="0.5" unit="second" maxnumber="500"></users>
    </arrivalphase>


    <arrivalphase phase="3" duration="2" unit="minute">
      <users interarrival="0.1" unit="second" maxnumber="500"></users>
    </arrivalphase>


    <arrivalphase phase="4" duration="10" unit="minute">
      <users interarrival="2" unit="second" maxnumber="500"></users>
    </arrivalphase>


    <arrivalphase phase="5" duration="5" unit="minute">
      <users interarrival="0.1" unit="second" maxnumber="500"></users>
    </arrivalphase>


    <arrivalphase phase="6" duration="10" unit="minute">
      <users interarrival="1" unit="second" maxnumber="500"></users>
    </arrivalphase>
  </load>

